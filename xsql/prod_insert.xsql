<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../xsl/prod_new.xsl"?>

<!--text/xsl-->
<page connection="{@connection}" xmlns:xsql="urn:oracle-xsql">
<xsql:query rowset-element="PRODUCT_CATEGORIES"
    row-element="CATEGORY">
   SELECT pc.id,
	pc.name
    FROM product_category pc
  ORDER BY pc.id     
</xsql:query>

</page>